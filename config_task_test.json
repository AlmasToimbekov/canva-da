{
  "start": {
    "type": "knot",
    "appendedParameters": {
      "projectId": "#PROJECT_ID#",
      "dataset": "#DATASET#",
      "bucket": "#GCS_BUCKET#",
      "saSource1": "sa360_canva_apac.Conversion_21700000001677017",
      "saSource2": "sa360_canva_latam.Conversion_21700000001677020",
      "saSource3": "sa360_canva_na.Conversion_21700000001677014",
      "saSource4": "sa360_canva_latam.Conversion_21700000001677020"
    },
    "next": "get_users"
  },
  "get_users": {
    "type": "query",
    "source": {
      "file": {
        "projectId": "${projectId}",
        "bucket": "${bucket}",
        "name": "sql/query_DA.sql"
      }
    },
    "destination": {
      "table": {
        "projectId": "${projectId}",
        "datasetId": "${dataset}",
        "tableId": "double_activation_gclids_${partitionDay}"
      },
      "writeDisposition": "WRITE_TRUNCATE"
    },
    "next": ["export_users", "save_user_ids"]
  },
  "export_users": {
    "type": "export",
    "source": {
      "projectId": "${destinationTable.projectId}",
      "datasetId": "${destinationTable.datasetId}",
      "tableId": "${destinationTable.tableId}",
      "location": "#DATASET_LOCATION#"
    },
    "destination": {
      "bucket": "${bucket}",
      "name": "#OUTBOUND#/API[CM]_config[GA4]_size[4]_${partitionDay}.ndjson"
    },
    "options": {
      "destinationFormat": "NEWLINE_DELIMITED_JSON",
      "printHeader": false
    }
  },
  "save_user_ids": {
    "type": "query",
    "source": {
      "file": {
        "projectId": "${projectId}",
        "bucket": "${bucket}",
        "name": "sql/save_user_ids.sql"
      }
    },
    "destination": {
      "table": {
        "projectId": "${projectId}",
        "datasetId": "${dataset}",
        "tableId": "double_activation_users_${partitionDay}"
      },
      "writeDisposition": "WRITE_TRUNCATE"
    }
  }
}
